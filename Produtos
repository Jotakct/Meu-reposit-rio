CREATE TABLE TB_clientes(
	endereco TEXT not NULL;
  	telefone TEXT not NULL;
  	nome_responsavel VARCHAR not NULL;
  	ramo_ativid TEXT not NULL;
  	razao_social TEXT not NULL;
  	CPNJ TEXT NOT NULL;
  	nome_fantasia TEXT not NULL;
   	id_clientes VARCHAR not NULL;
  	CONSTRAINT id_clientes PRIMARY KEY(id_clientes);


create table TB_pedidos(
	data_pedido TEXT not NULL,
  	forma_pagamento TEXT not NULL
  	data_entrega_pr text not NULL,
  	data_entrega_ef TEXT not NULL,
  	id_pedidos VARCHAR not NULL,
  	CONSTRAINT id_pedidos PRIMARY KEY(id_pedidos, fk_id_clientes),
  	fk_id_clientes VARCHAR REFERENCES id_clientes (tb_clientes),   
);
  
  CREATE TABLE REL_itens(
  	fk_id_pedidos VARCHAR NOT NULL, 
  	fk_id_produto VARCHAR not NULL,
 	FOREIGN key (fk_id_pedidos) REFERENCES tb_pedidos(id_pedidos),
  	FOREIGN key (fk_id_produto) REFERENCES tb_produto(id_prduto)
);

create TABLE TB_produto(
	id_produto  VARCHAR not NULL,
  	CONSTRAINT id_produto PRIMARY key(id_produto),
  	nome_ produto TEXT not NULL,
  	tamanho_produto SMALLINT not NULL,
  	cor_produto INTEGER not NULL,
  	pre√ßo_produto INTEGER not NULL

);
  create TABLE TB_estoque_produto(
  		fk_id_protudo TEXT not NULL,
    	FOREIGN KEY (fk_id_produto) REFERENCES tb_produto(id_produto),
   		CONSTRAINT fk_id_produto primary KEY(fk_id_produto), 
 		Qt_estoque INTEGER not NULL	 
 	 );
  CREATE TABLE REL_Mprima_produto(
  		fk_id_produto TEXT not NULL,
    	FOREIGN key (fk_id_produto) REFERENCES tb_produto(id_produto),
   		fk_id_Mprima_produto TEXT not NULL,
    	FOREIGN KEY (fk)
    	CONSTRAINT id_Mprima_produto PRIMARY key(id_Mprima_produto)
  );
  create TABLE Tb_producao(
  		Qt_produzida INTEGER not NULL,
    	custo_lote INTEGER not NULL,
    	data_enterga INTEGER not NULL,
    	id_lote INTEGER NOT NULL;
    	id_produto TEXT not NULL,
    	FOREIGN KEY id_produto REFERENCES tb_produto(id_produto),
    	CONSTRAINT id_lote PRIMARY key(id_lote,id_produto);   	
  );
  create table TB_Mprima(
  		nome_Mprima TEXT not NULL,
    	descricao TEXT not NULL,
    	Qt_estoque INT not NULL,
    	
  );
